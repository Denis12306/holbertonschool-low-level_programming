#include "main.h"
#include <stdio.h>
/**
 * _strspn - Write a function that gets the length of a prefix substring.
 * @s is the string
 * @accept is also a string
 * Return: s or NULL
 */
unsigned int _strspn(char *s, char *accept)
{
	char *start = accept;

	while (*s)
	{
		while (*accept)
		{
			if (*accept == *s)
				return (s);
			accept++;
		}

		accept = start;
		s++;
	}
	return (NULL);
}
